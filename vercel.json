{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js", "methods": ["GET", "POST", "OPTIONS"] }
  ],
  "env": { "NODE_ENV": "production" },
  "functions": {
    "index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}